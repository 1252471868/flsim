{
    "clients": {
        "total": 3,
        "per_round":1,
        "label_distribution": "uniform",
        "do_test": false,
        "test_partition": 0.2     
    },
    "data": {
        "loading": "dynamic",
        "partition": {
            "size": 600,
            "range": [
                50,
                200
            ]
        },
        "IID": false,
        "bias": {
            "primary": 0.8,
            "secondary": false
        }
    },
    "federated_learning": {
        "rounds": 200,
        "target_accuracy": 0.98,
        "task": "train",
        "epochs": 5,
        "batch_size": 4
    },
    "model": "MNIST",
    "paths": {
        "data": "./data",
        "model": "./models",
        "reports": "reports.pkl"
    },
    "server": {
        "type":"marl",
        "socket": {
            "ip": "192.168.2.12",
            "port":5050,
            "protocol": "udp"
        }
    },
    "marl": {

        "n_episodes":15,
        "n_steps":3,
        "n_eval_steps": 50,
        "n_actions":2,
        "n_agents":3,
        "epsilon_anneal_scale":"episode",
        "epsilon": 1,
        "anneal_epsilon": 0.3,
        "min_epsilon": 0.01,
        "hidden_dim": 64,
        "buffer_size": 16,
        "batch_size": 3,
        "reuse_network": true,
        "train_steps":10,
        "save_cycle": 5,
        "load_model": false,
        "lr": 0.001,
        "gamma": 0.9,
        "grad_norm_clip": 10,
        "target_update_cycle":2,
        "model_dir": "output/marl_models/4"

    },
    "dqn":{}
}
