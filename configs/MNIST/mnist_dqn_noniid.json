{
    "clients": {
        "total": 2,
        "per_round":1,
        "label_distribution": "uniform",
        "do_test": false,
        "test_partition": 0.2     
    },
    "data": {
        "loading": "dynamic",
        "partition": {
            "size": 300,
            "range": [
                50,
                200
            ]
        },
        "IID": false,
        "bias": {
            "primary": 0.8,
            "secondary": false
        }
    },
    "federated_learning": {
        "rounds": 100,
        "target_accuracy": 0.99,
        "task": "train",
        "epochs": 5,
        "batch_size": 4
    },
    "model": "MNIST",
    "paths": {
        "data": "./data",
        "model": "./models",
        "reports": "reports.pkl"
    },
    "server": {
        "type":"dqntrain",
        "socket": {
            "ip": "10.0.0.13",
            "port":5000
        }
    },
    "dqn": {
        "episode": 150,
        "max_steps": 30,
        "learning_rate": 0.0001,
        "gamma": 0.95,
        "epsilon_initial": 1.0,
        "epsilon_decay": 0.95,
        "epsilon_min": 0.01,
        "memory_size": 200,
        "batch_size": 4,
        "target_update": 10,
        "hidden_layers": [256, 256],
        "reward_xi": 64,
        "rewards_log": "output/dqn_model_rewards.csv",
        "saved_model": "output/dqn_models/dqn_model",
        "trained_model": "output/dqn_models/dqn_model_200.h5",
        "pca_model": "output/dqn_models/pca.pkl"
    }
}
